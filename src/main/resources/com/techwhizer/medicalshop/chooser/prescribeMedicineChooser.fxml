<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>

<BorderPane maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="400.0" prefWidth="491.0"
            style="-fx-border-color: grey;" xmlns="http://javafx.com/javafx/20.0.1"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.techwhizer.medicalshop.controller.chooser.PrescribeMedicineChooser">
    <center>
        <VBox alignment="CENTER" BorderPane.alignment="CENTER">
            <children>
                <TableView fx:id="tableView" prefHeight="334.0" prefWidth="410.0" VBox.vgrow="ALWAYS">
                    <columns>
                        <TableColumn fx:id="colSrNo" maxWidth="70.0" minWidth="50.0" prefWidth="-1.0" text="SR #" />
                        <TableColumn fx:id="colInvoiceNum" maxWidth="140.0" minWidth="130.0" prefWidth="-1.0" text="Invoice #" />
                  <TableColumn fx:id="colConsultDate" maxWidth="140.0" minWidth="150.0" prefWidth="-1.0" text="Consult Date" />
                        <TableColumn fx:id="colAction" maxWidth="90.0" minWidth="90.0" prefWidth="-1.0" text="ACTION" />
                    </columns>
                    <columnResizePolicy>
                        <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                    </columnResizePolicy>
                </TableView>
            </children>
        </VBox>
    </center>
</BorderPane>
